{
  "name": "browserify-boilerplate",
  "version": "0.0.1",
  "description": "A browserify boilerplate with watchify for quick web app development",
  "main": "index.js",
  "scripts": {
    "build": "gulp prod",
    "codestyle": "standard",
    "dev": "gulp dev",
    "postinstall": "npm run build -s",
    "prepush": "npm run codestyle",
    "serve": "npm run build -s && node index.js",
    "start": "node index.js",
    "test": "tape ./test/unit/{**,**/**}.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "keywords": [
    "browserify"
  ],
  "author": "gimeno.sergio@gmail.com",
  "devDependencies": {
    "browser-sync": "^2.12.1",
    "browserify": "^9.0.8",
    "bulkify": "^1.2.0",
    "conventional-changelog": "^1.1.0",
    "del": "^1.1.1",
    "envify": "^3.4.0",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-changed": "^1.3.0",
    "gulp-connect": "^3.2.2",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "husky": "^0.12.0",
    "run-sequence": "^1.1.5",
    "standard": "^8.6.0",
    "tape": "^4.6.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "compression": "^1.4.3",
    "express": "^4.12.3",
    "morgan": "^1.5.2",
    "snyk": "^1.30.1"
  },
  "snyk": true
}
